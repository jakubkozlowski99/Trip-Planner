@model Test_web_app.Models.Trip

@using (Html.BeginForm("Add", "Planner", FormMethod.Post))
{
    @Html.ValidationSummary()
    <div>Nazwa: </div>@Html.TextBoxFor(x => x.Title)
    <div>Opis: </div>@Html.TextBoxFor(x => x.Description)
    <input type="date" name="startDate" id="startDate">
    <input type="date" name="endDate" id="endDate">
    <button type="submit">Dodaj wycieczkę</button>
}

